          in_array("on", $struct[$key]['values'], true) &&
          in_array("off", $struct[$key]['values'], true)) {
         $struct[$key]["appval"] = ($values[$key] ? "on" : "off");
         }
      elseif (is_array($struct[$key]['values']) &&
      	      in_array("on", $struct[$key]['values'], true) &&
              ($values[$key] === "1" || $values[$key] === "")) {
         $struct[$key]["appval"] = ($values[$key] === "1" ? "on" : "off");
         }
      else {
         $struct[$key]["appval"] = $values[$key];
         }
      $struct[$key]["setval"] = $struct[$key]["appval"];
      }
   else {
      $struct[$key]["appval"] = null;
      unset($struct[$key]["setval"]);
      }
   }
return ($struct);
	} //|o2_fine_exp|

