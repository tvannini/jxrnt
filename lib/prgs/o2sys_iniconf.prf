foreach ($struct as $key => $info) {
   $quote = "";
   if (isset($struct[$key]["setval"])) {
      $default    = $struct[$key]["default"];
      if (!(is_numeric($default) || ($default === "on") || ($default === "off"))) {
         $quote = '"';
         }
      $list[$key] = $quote.$struct[$key]["setval"].$quote;
      $maxlen     = max(strlen($key), $maxlen);
