function o2sys_iniconf_exp_3() {
	$search = trim(o2val('prg§_§var','search'));
/* All items */
if ($search) {
   $list = array();
   foreach (o2val('prg§_§var','ini_struct') as $item => $info) {
      if (stripos($info["desc"], $search) !== false) {
         $list[$item] = $item;
         }
      }
   }
/* Filtered items */
else {
   $words = array_keys(o2val('prg§_§var','ini_struct'));
   $list = array_combine($words, $words);
   }
return ($list);
	} //|o2_fine_exp|

