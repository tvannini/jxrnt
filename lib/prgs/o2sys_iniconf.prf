function o2sys_iniconf_exp_26() {
	$rnt_root = new o2_dir(o2rnt_root());
$ini      = $rnt_root."janox.ini";
$values   = parse_ini_file($ini);
$struct   = o2val('prg§_§var','ini_struct');
foreach (array_keys($struct) as $key) {
   if (isset($values[$key])) {
      if (is_array($struct[$key]['values']) &&
          count($struct[$key]['values']) == 2 &&
          in_array("on", $struct[$key]['values']) &&
          in_array("off", $struct[$key]['values'])) {
         $struct[$key]["rntval"] = ($values[$key] ? "on" : "off");
         }
      else {
         $struct[$key]["rntval"] = $values[$key];
         }
      }
   else {
      $struct[$key]["rntval"] = null;
      }
   }
return ($struct);
	} //|o2_fine_exp|

