   else {
      if (isset($list[$key])) {
         $initxt.= $key.str_repeat(" ", ($maxlen - strlen($key)))." = ".
                   $list[$key]."\n";
         }
      }
   }
file_put_contents(o2val('prg§_§var','ini_file'), $initxt);
return (true);
	} //|o2_fine_exp|

