function jxprocesses_exp_1() {
	$user   = o2app_user();
$user   = ($user == 'root' || o2par(1) ? '' : $user);
$job    = (o2par(4) ? o2par(4) : o2val('prg§_§var','flt_job'));
$status = o2val('prg§_§var','flt_status');
$days   = o2val('prg§_§var','days_filter'); 
$code   = o2_run_job::get_monitor($user, 
                                  'refresh', 
                                  'refresh', 
                                  'btn_click',
                                  $job,
                                  $status,
                                  $days);
return ($code);
	} //|o2_fine_exp|

