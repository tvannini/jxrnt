function jxtranslate_exp_44() {
	$run_job = o2val('prg§_§var','proc_do_all'); 
if ($run_job) {
   $rnt    = $GLOBALS['o2_runtime'];
   $imgs   = $rnt->alias."img/jobs/";
   $run    = $run_job->is_running();
   $wait   = ($run_job->progress < 1);
   $error  = ($run_job->error || 
              (($run_job->progress < 100 || $run_job->status == 'R') && 
               !$run) ? true : false);
   $status = ($wait ? 'Waiting' : 
              ($error ? 'Error' : o2format($run_job->status, 'jxjob_status')));
   $ico    = $imgs.
             ($wait ? 'scheduled.png' :
              ($error ? 'failed.png' : 
               ($run_job->status == 'C' ? 'executed.png' : 'running.png')));                          
   $bar    = o2job_get_bar($run_job->id, 'get_processes', 'get_processes'); 
   $blank  = "&nbsp;&nbsp;&nbsp;&nbsp;- - -";
   $code   = '<table style="width:100%"><tr><td><img class="jximgbtn" src="'.$ico.
             '" onclick="'.o2_act4js(0, "proc_detail", $run_job->id).
             '"></td><td>'.$status.'</td><td>'.
             ($run_job->start > 0 ? date("d/m/y H:i", $run_job->start) : $blank).
             "</td><td>".($run_job->end > 0 ? date("d/m/y H:i", $run_job->end) : $blank).
             "</td></tr><tr><td colspan='4' style='width:100%;height:18px;'>".$bar.
             "</td></tr></table>\n";
   }
else {
   $code = '';
   }
return ($code);
	} //|o2_fine_exp|

