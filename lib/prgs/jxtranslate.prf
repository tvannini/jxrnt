function jxtranslate_exp_38() {
	$mode = o2val('prg§_§var','prod_mode');
if (!o2job_get_by_name('jxmlt_produce')) {
   o2job_create('jxmlt_produce',
                'tools/jxmlt_produce',
                false,
                'Data and file production for Multilanguage Translation',
                30);
   }
o2job_run('jxmlt_produce', 
          array('mode'        => $mode,
                'prg'         => ($mode == 'P' ? o2val('prg§_§var','prg') : ''),
                'lang_from'   => o2val('prg§_§var','lang_from'),
                'lang_to'     => o2val('prg§_§var','lang_to'),
                'use_default' => o2val('prg§_§var','check_default')));
return (true);
	} //|o2_fine_exp|

