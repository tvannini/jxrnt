function jxtranslate_exp_41() {
	$m = o2val('prg§_§var','harvest_mode');
$e = !o2zero('prg§_§var','lang_to') && 
     (($m == 'P' && !o2zero('prg§_§var','prg')) || 
      ($m == 'M' && !o2zero('prg§_§var','menu')) || 
      ($m == 'U') ||
      ($m == 'R'));
/* ________________________________________________ Check process running ___ */
$p = o2val('prg§_§var','proc_do_all');
if ($p) {
   $e = $e && !$p->is_running();
   }
return ($e);
	} //|o2_fine_exp|

