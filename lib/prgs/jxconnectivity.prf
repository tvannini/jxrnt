   // ___________________________________ Fix record layout errors (rebuild) ___
   foreach ($errors_tab as $tab) {
      if ($tab[0] !== 'jxresultset' && $tab[3] && $tab[3] == 'error') {
         o2tab_fkeys_setoff($tab[0]);
         if (o2tab_rebuild($tab[0])) {
            $list[$tab[0]] = array('jxresultset', $tab[0], $img_path.'tab.png');
            $tabs++;
            $tot++;
            foreach ($errors_tab as $rt_key => $r_tab) {
               if ($r_tab[3] && $r_tab[3] == 'index') {
                  unset($errors_tab[$rt_key]);               
                  }            
               }
            }
         else {
            $list[$tab[0]] = array('jxresultset', $tab[0], $img_path.
                                                           'tab_error.png');
            $tot++;
            }
         }
      }
   // ___________________________________________________ Fix indexes errors ___      
   foreach ($errors_tab as $tab) {
      if ($tab[0] !== 'jxresultset' && $tab[3] && $tab[3] == 'index') {
         o2tab_indexes_setoff($tab[0]);
         if (o2tab_indexes_seton($tab[0])) {            
            $list[$tab[0]] = array('jxresultset', $tab[0], $img_path.'tab.png');
            $tabs++;
            $tot++;
            }
         else {
            $list[$tab[0]] = array('jxresultset', $tab[0], $img_path.
                                                           'tab_error.png');
            $tot++;
            }
         }
      }
   // _____________________________________________ Fix foreign keysa errors ___      
