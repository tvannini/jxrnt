$to_date   = o2val('prg§_§var','date_to');
$to_time   = o2val('prg§_§var','time_to');
if ((($to_date > '00000000') && ($to_date < $from_date)) ||
    (($to_time > '000000') && ($to_date == $from_date) && ($to_time < $from_time))) {
   $cond = true;
   }
return ($cond);
	} //|o2_fine_exp|

